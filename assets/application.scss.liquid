body {
	position: relative;
	min-height: 100vh;
	overflow-x: hidden;
}

.nav-holder .logo{
	color: black;
	text-decoration: none;
}
.nav-holder .logo:hover{
	text-decoration: none;
	color: black;
}
#page-container {
	position: relative;
	min-height: 100vh;
}

#content-wrap { 
	padding-bottom: 2.5rem; /* Footer height */
}

#footer {
	position: absolute;
	bottom: 0;
	width: 100%;
	height: 2.5rem; /* Footer height */
}



button:focus {
	outline: 6px double #FFC55F !important;
}
:focus {
	outline: 6px double #FFC55F  !important;
}

.logo:focus{
	outline: none !important;
}
header {
	margin-bottom: 10px;
}

.nav-holder {
	position: relative;
	display: grid;
    justify-content: center;
	margin-top: 1.5rem;
}
.cart-active{
	display: none;
}

.logo {
  
	justify-content: center;
	display: flex;
	z-index: 1000;
}
.logo .nav-logo{
	font-family: "Sacramento", cursive;
	text-transform: capitalize;
	color: black;

}
.footer--logo .flogo{
	font-family: "Sacramento", cursive;
	text-transform: capitalize;
	color: white;

}

.nav__button:hover {
	cursor: pointer;
}

.nav-cart {
	padding-right: 20px;
}

.nav-links {
	display: block;
	width: 0px;
	left: 0px;
	top: 40px;
	position: absolute;
	z-index: 100;
	
	
}
.nav__button {
	position: absolute;
	left: 0px;
	top: 0px;
}
.burger-active {
	left: 200px;
	transition: all 0.5s cubic-bezier(0.6, 0.05, 0.28, 0.91);
}


.nav-linkslist {
	color: transparent;
	text-decoration: none !important;
}

.nav-linkslist:hover {
	color: transparent;
	text-decoration: none !important;
}




.menu-active {
	width: 200px;
	display: block;
	position: fixed;
	top: 0px;
	left: -5px;
	background: #f1f1f1;
	height: 100%;
	transition: width 0.5s cubic-bezier(0.6, 0.05, 0.28, 0.91);	
}
.menu-active .nav-items li .nav-linkslist{
	text-decoration: none ;
	color: black ;
}

.main-active {
	position: relative;
	left: 200px;
	
	
}

@media (min-width: 768px) {

	.cart-active{
		display: block;
	}
	.nav__button {
		display: none;
	}

	.nav-items {
		display: flex;
		justify-content: center;
	}

	.nav-links {
		display: flex;
		align-items: baseline;
	}

	.nav-links {
		position: relative;
		top: auto;
		left: auto;
		display: contents;
		margin: 0 auto;
		justify-content: center;
	}
	.nav-holder{
		grid-template-columns: 1fr;
	}
	

	.menu-active {
		width: auto;
		display: flex;
		position: relative;
		top: auto;
		left: auto;
		background: transparent;
		height: auto;
	}

	.main-active {
		left: auto;
	}
	.nav-linkslist{
		color: black

	}
	.nav-linkslist:hover {
		color: black;
		text-decoration: none !important;
       outline: 5px solid #ffa5cb;
       padding: 5px;
		outline-style: double;
		transition: all .09s;
	}
}

@media (min-width: 1200px) {
	.nav-holder {
		display: flex;
		justify-content: space-around;
		align-items: center;
	}
	.nav-items {
		margin: 0 auto;
	}
	.logo {
		padding-left: 5%;
		position: absolute;
		left: 0px;
	}
	.nav-cart {
		padding-right: 20px;
		position: absolute;
		right: 0px;
	}
	.nav-links {
		display: flex;
		align-items: center;
	}
}





.collapsible {
	font-weight: 500;
	color: rgb(0, 0, 0);
	cursor: pointer;
	padding: 8px;
	width: 100%;
	border: none;
	text-align: left;
	outline: none;
	background-color: transparent;
	border-top: 1px solid #eee;
	border-bottom: 1px solid #eee;
	font-size: 16px;
	text-transform: uppercase;
}
.collapsible::before {
	content: "+";
	padding-right: 1em;
}

.test-active::before {
	content: "-";
	padding-right: 1em;
}

