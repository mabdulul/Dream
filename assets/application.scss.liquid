.accordion__button{
	display: block;
	width: 100%;
	padding: 15px;
	border: none;
	outline: 6px double #FFC55F;
	font-family: "Josefin Sans", sans-serif;
	font-size: 18px;
	color: black;
	text-align: left;
	margin-bottom: 20px;
	background-color: transparent;
}

.accordion__button--active{
	margin-bottom: 0px;
	
	
}
.accordion__button::after{
	content: "+";
	float: right;
	transform: scale(1.5);
}

.accordion__button--active::after{
	content: "-";
	float: right;
	transform: scale(1.5);

}



.accordion__content{
	overflow: hidden;
	max-height: 0;
	transition: max-height 0.2s;
	padding: 0 15px;
	font-family: "Josefin Sans", sans-serif;
	font-weight: 300;
	
	margin: 20px 0px ;
}



	
	