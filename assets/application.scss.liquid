.collage {
	display: grid;
	width: 100%;
	height: 100%;
	grid-template-columns: repeat(2, 1fr);
	grid-template-rows: repeat(4, 250px);
}
.collage--single {
	background-size: cover;
	width: 100%;
	background-position: center;
	padding: 150px;
}
.collage .collage__img-1 {
	background-position: center;
	background-repeat: no-repeat;
	background-color: #d0c3f1;
	grid-column: 1 / span 2;
	grid-row: 1 / span 2;
}
.collage .collage__img-2 {
	background-size: cover;
	background-position: center;
	background-position: bottom;
}
.collage .collage__img-3 {
	background-position: center;
	background-size: contain;
	background-repeat: no-repeat;
	background-color: #ec99ad;
}
.collage .collage__img-4 {
	background-size: contain;
	background-repeat: no-repeat;
	background-position: center;
}
.collage .collage__img-5 {
	background-size: cover;
	background-position: center;
}
.collage .collage__img-6 {
	background-size: cover;
	background-position: center;
}

@media (min-width: 991.98px) {
	.collage {
		display: grid;
		width: 100%;
		height: 100%;
		grid-template-columns: repeat(5, 1fr);
		grid-template-rows: repeat(2, 250px);
	}
	.collage .collage__img-1 {
		background-position: center;
		background-repeat: no-repeat;
		background-color: #d0c3f1;
		grid-column: 1/3;
		grid-row: 1 / span 2;
	}
	.collage .collage__img-2 {
		background-size: cover;
		background-position: center;
		grid-column: 3 / span 1;
		grid-row: 1;
		background-position: bottom;
	}
	.collage .collage__img-3 {
		grid-column: 4 / span 1;
		grid-row: 1;
	}
	.collage .collage__img-4 {
		background-image: url(./images/dee.png);
		grid-column: 5 / span 1;
		grid-row: 1;
	}
	.collage .collage__img-5 {
		background-size: cover;
		background-position: center;
		grid-column: 3 / span 1;
		grid-row: 2;
	}
	.collage .collage__img-6 {
		background-size: cover;
		background-position: center;
		grid-column: 4 / span 2;
		grid-row: 2;
	}
}
